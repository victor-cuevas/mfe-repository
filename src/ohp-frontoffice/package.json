  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "ngcc": "ngcc",
    "nx": "nx",
    "api": "json-server mock-api/db.json --routes mock-api/routes.json --watch --no-cors=true --middlewares mock-api/middleware.js --disable-web-security",
    "start": "ng serve --proxy-config proxy.conf.json",
    "start:portal:mocks": "ng serve mfe-broker-portal --open --configuration=mocks",
    "start:panel:mocks": "ng serve mfe-broker-panel --open --configuration=mocks",
    "build": "ng build",
    "build:brokerportal": "nx build mfe-broker-portal --prod",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "nx run mfe-broker-cy:cy-test",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js",
    "swagger:preinstall": "npm config set script-shell \"C:\\Program Files\\git\\bin\\bash.exe\"",
    "swagger:generate": "bash ./tools/generators/swagger/swagger.sh",
    "affected:upload:test": "nx affected --target=upload-april-test --base=HEAD~1 --head=HEAD || { echo 'nx affected:build failed'; exit 1; }",
    "affected:upload:prod": "nx affected --target=upload --base=HEAD~1 --head=HEAD --with-deps --prod || { echo 'nx affected:build failed'; exit 1; }",
    "affected:deploy:prod": "nx affected --target=deploy --base=remotes/origin/main~1 --head=HEAD --with-deps --prod || { echo 'nx affected:build failed'; exit 1; }"
  },
