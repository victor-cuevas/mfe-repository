{
  "projectType": "library",
  "root": "libs/shared/i18n",
  "sourceRoot": "libs/shared/i18n/src",
  "prefix": "close-front-office",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/shared/i18n"],
      "options": {
        "jestConfig": "libs/shared/i18n/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/shared/i18n/src/**/*.ts", "libs/shared/i18n/src/**/*.html"]
      }
    }
  }
}
